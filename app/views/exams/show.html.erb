<p id="notice"><%= notice %></p>

<p>
  <b>Subject:</b>
  <%= @exam.subject %>
</p>

<p>
  <b>Name:</b>
  <%= @exam.name %>
</p>

<p>
  <b>Date:</b>
  <%= @exam.date %>
</p>
<p>
  <b>Źródła do nauki:</b>
  <% @exam.resources.each do |resource| %>
    <%= resource.name %>
    <br />
    Zawartość: <%= resource.content %>
    <br />
    Link: <%= link_to(resource.name, resource.url) %>
<% end %>
</p>


<%= link_to 'Edit', edit_exam_path(@exam) %> |
<%= link_to 'Back', exams_path %>
